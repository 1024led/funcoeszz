#!/bin/bash
debug=0
values=1

echo -e '\033[10;750]\033[11;100]\a' > _tmp1

(
echo -n "Vou bipar em 0 minutos... "
echo -ne '\033[11;900]'
echo -ne "\033[10;500]\a"
echo -ne "\033[10;400]\a"
echo -ne "\033[10;500]\a"
echo -ne "\033[10;400]\a"
echo -ne '\033[10;750]\033[11;100]'
echo OK)  > _tmp2

tests=(
''	t	$(echo -ne '\033[10;750]\033[11;100]\a')
''	a	_tmp1
0	a	_tmp2
)
. _lib
